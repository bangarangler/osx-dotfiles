let SessionLoad = 1
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/Desktop/svelteClassless
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +40 term://.//26029:ranger\ --choosefiles=/tmp/chosenfile\ --selectfile=\"src/template.html\"
badd +40 term://.//96539:ranger\ --choosefiles=/tmp/chosenfile\ --selectfile=\"static/customGlobal.css\"
argglobal
%argdel
set splitbelow splitright
wincmd t
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
argglobal
if bufexists("term://.//96539:ranger\ --choosefiles=/tmp/chosenfile\ --selectfile=\"static/customGlobal.css\"") | buffer term://.//96539:ranger\ --choosefiles=/tmp/chosenfile\ --selectfile=\"static/customGlobal.css\" | else | edit term://.//96539:ranger\ --choosefiles=/tmp/chosenfile\ --selectfile=\"static/customGlobal.css\" | endif
setlocal fdm=indent
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=99
setlocal fml=1
setlocal fdn=10
setlocal nofen
let s:l = 40 - ((39 * winheight(0) + 20) / 40)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
40
normal! 0
tabnext 1
if exists('s:wipebuf') && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 winminheight=1 winminwidth=1 shortmess=filnxtToOFIc
let s:sx = expand("<sfile>:p:r")."x.vim"
if file_readable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &so = s:so_save | let &siso = s:siso_save
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
